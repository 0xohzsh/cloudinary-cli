# Cloudinary Configuration
# Replace these values with your actual Cloudinary credentials
CLOUDINARY_CLOUD_NAME=your_cloud_name_here
CLOUDINARY_API_KEY=your_api_key_here
CLOUDINARY_API_SECRET=your_api_secret_here
CLOUDINARY_SECURE=true

# Default folder for uploads (optional)
# Leave empty to use root folder, or set to a folder name like 'Downloads'
# Examples: 'Downloads', 'my-projects', 'backups', or leave empty for root
CLOUDINARY_DEFAULT_FOLDER=Downloads

# Unique filename generation (optional)
# true: Cloudinary generates unique names with random suffixes (e.g., image_abc123)
# false: Use exact filenames without random suffixes (e.g., image)
# Examples: true, false
CLOUDINARY_UNIQUE_NAMES=false

# Maximum file size in MB before compression (default: 8MB)
# Set based on your Cloudinary plan limits
CLOUDINARY_MAX_FILE_SIZE=8
